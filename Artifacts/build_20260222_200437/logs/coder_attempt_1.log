+
+        return ScoreResult(
+            dis: disScore,
+            ads: adsScore,
+            eis: eisScore,
+            cis: cisScore,
+            oes: oesScore,
+            coi: coiScore
+        )
+    }
+
+    private func clamp(_ value: Double) -> Double {
+        min(max(value, -1.0), 1.0)
+    }
+}
