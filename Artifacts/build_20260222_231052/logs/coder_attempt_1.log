                try data.write(to: url, options: [.atomic])

                exportedItem = ExportedItem(url: url, type: format.utType)
            } catch {
                self.error = error
            }
            isGenerating = false
        }
    }
}

struct ExportedItem: Identifiable {
    let id = UUID()
    let url: URL
    let type: UTType
